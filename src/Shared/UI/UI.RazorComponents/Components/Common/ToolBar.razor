<MudToolBar Dense Class="ors-form-command-panel">

    @foreach (var command in Commands)
    {
        <MudTooltip Text=@command.Tooltip>
            <MudIconButton Icon="@command.Icon" Color="Color.Inherit" Class=@($"mr-5 {command.CssClass}") OnClick="@(command.Callback)" />
        </MudTooltip>
    }

 </MudToolBar>


    